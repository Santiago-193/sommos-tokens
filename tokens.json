{
  "global": {
    "12": {
      "value": "$root * 1.5",
      "type": "sizing"
    },
    "16": {
      "value": "$root * 2",
      "type": "sizing"
    },
    "20": {
      "value": "$root * 2.5",
      "type": "sizing"
    },
    "24": {
      "value": "$root * 3",
      "type": "sizing"
    },
    "28": {
      "value": "$root * 3.5",
      "type": "sizing"
    },
    "30": {
      "value": "$root * 3.75",
      "type": "sizing"
    },
    "32": {
      "value": "$root * 4",
      "type": "sizing"
    },
    "40": {
      "value": "$root * 5",
      "type": "sizing"
    },
    "root": {
      "value": "8",
      "type": "other",
      "description": "valor root"
    },
    "acent": {
      "100%": {
        "value": "#58C281",
        "type": "color",
        "description": "Color para acentuar objetos interactivos"
      },
      "20%": {
        "value": "rgba({acent.100%}, 0.2)",
        "type": "color",
        "description": "Acento desactivado"
      }
    },
    "primary": {
      "100%": {
        "value": "#0A1810",
        "type": "color",
        "description": "Para textos y otra informaci√≥n"
      },
      "12%": {
        "value": "rgba({primary.100%}, 0.12)",
        "type": "color"
      }
    },
    "system": {
      "white": {
        "value": "#fff",
        "type": "color"
      },
      "error": {
        "value": "#FE5353",
        "type": "color"
      },
      "warning": {
        "value": "#FECE53",
        "type": "color"
      }
    },
    "font": {
      "primary": {
        "value": "Poppins",
        "type": "fontFamilies"
      }
    },
    "body": {
      "size": {
        "regular": {
          "value": "$16",
          "type": "fontSizes"
        },
        "h2": {
          "value": "$20",
          "type": "fontSizes"
        },
        "h1": {
          "value": "$40",
          "type": "fontSizes"
        },
        "info": {
          "value": "$12",
          "type": "fontSizes"
        }
      },
      "regular": {
        "value": {
          "fontFamily": "$font.primary",
          "fontSize": "{body.size.regular}",
          "fontWeight": "{body.weigth.regular}"
        },
        "type": "typography"
      },
      "subtitle": {
        "value": {
          "fontFamily": "$font.primary",
          "fontSize": "{body.size.h2}",
          "fontWeight": "$body.weigth.medium"
        },
        "type": "typography"
      },
      "weigth": {
        "regular": {
          "value": "Regular",
          "type": "fontWeights"
        },
        "medium": {
          "value": "Medium",
          "type": "fontWeights"
        }
      },
      "title": {
        "value": {
          "fontFamily": "$font.primary",
          "fontWeight": "$body.weigth.medium",
          "fontSize": "$body.size.h1"
        },
        "type": "typography"
      },
      "info": {
        "value": {
          "fontFamily": "$font.primary",
          "fontWeight": "$body.weigth.regular",
          "fontSize": "$body.size.info"
        },
        "type": "typography"
      }
    },
    "padding": {
      "xsmall": {
        "value": "$root",
        "type": "spacing"
      },
      "small": {
        "value": "$12",
        "type": "spacing"
      },
      "medium": {
        "value": "$16",
        "type": "spacing"
      },
      "xl": {
        "value": "$40",
        "type": "spacing"
      }
    },
    "margin": {
      "xsmall": {
        "value": "$root",
        "type": "spacing"
      },
      "small": {
        "value": "$12",
        "type": "spacing"
      },
      "medium": {
        "value": "$16",
        "type": "spacing"
      },
      "xl": {
        "value": "$40",
        "type": "spacing"
      }
    },
    "border": {
      "value": "$root / 2",
      "type": "borderRadius"
    }
  },
  "$themes": []
}